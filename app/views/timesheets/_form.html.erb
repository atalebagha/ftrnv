<%= simple_form_for(@timesheet) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :project_id %>
    <%= f.input :month %>
    <%= f.input :year %>
    <%= f.input :description %>
    <%= f.input :committed %>
    <%= f.input :committed_at %>
    <%= f.input :lock_version %>
    <%= f.input :start_day_cache %>
    <%= f.input :auto_sort %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
